<div class="row">

  <div class="span4 small-6 columns">
    <ol id="origin" class="simple_with_animation vertical">
      <% @meetups['results'].each do |meetup| %>
        <div class="meet">
          <% if meetup.has_key?("venue") %>
            <li class="event-name">
              <%= meetup['name'] %>
            </li>
            <li class="event-group">
            </li>
            <li class="event-venue">
              <%= meetup['venue']['name'] %>
            </li>
            <li class="event-time">
              <%= (meetup['time']) %>
            </li>
          <% end %>
        </div>
      <% end %>
    </ol>
  </div>

  <div class="span4 small-6 columns">
    <ol id="drop" class="simple_with_animation vertical">
      <% @meetups['results'].each do |meetup| %>
        <div class="meet">
          <% if meetup.has_key?("venue") %>
