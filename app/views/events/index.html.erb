
<div id="meetup_meetups_list">
  <% @meetups['results'].each do |meetup| %>
  <div class="meet small-6 columns left">
    <ul>
    <% if meetup.has_key?("venue") %>
      <li>
        <%= meetup['name'] %>
      </li>
      <li>
        <%= meetup['group']['name'] %>
      </li>
      <li>
        <%= meetup['venue']['name'] %>
      </li>
      <li>
        <%= meetup['time'].to_s.slice(0, 10) %>
      </li>
    <% end %>
    </ul>
  </div>
  <% end %>
</div>

<% if @current_user %>
  <div id="user_meetup_list" class="small-6 columns" style="background-color: yellow">
   <%= current_user.events.to_s %>
  </div>
  <% else %>
  <div id="blank_list" class="small-6 columns" style="background-color: blue, height 100%">

  </div>


<% end %>